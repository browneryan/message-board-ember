<div class="container">
  <div class="index-body">
    <div class="row">
      {{#each model.questions as |question|}}
        {{#link-to 'question' question.id}}
          <h5>Q: {{question.inquiry}}</h5>
          <p>By: {{question.author}}</p>
        {{/link-to}}
      {{/each}}
    </div>
    <div class="newQuestion">
      {{new-question save2="save3"}}
    </div>
  </div>
</div>
